ScreenManager:

  # main.py / home_screen.kv
  Main_Menu:

  # game.py / kv
  Game:

<Main_Menu>:
  name: 'main_menu_screen'
  MDFloatLayout:    
    MDFillRoundFlatIconButton:
      text: "Start"
      icon: "controller"
      text_color: "black"
